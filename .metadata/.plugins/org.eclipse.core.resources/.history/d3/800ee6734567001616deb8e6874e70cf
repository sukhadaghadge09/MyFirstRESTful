package org.sukhada.practice.messenger.service;


import java.util.ArrayList;
//import java.util.List;

import org.sukhada.practice.messenger.model.Message;

public class MessageService {
	
	public ArrayList<Message> getAllMessages()
	{
		Message msg1=new Message(1L,"Hello","author");
		Message msg2=new Message(2L,"Hello","author");
		ArrayList<Message> list=new ArrayList<Message>();
		//List<Message> list1=new ArrayList<>();
		list.add(msg1);
		list.add(msg2);
		
		return list;
		
	}

}
